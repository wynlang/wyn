// TDD Test: Comprehensive test 280

fn test_comprehensive_280() -> int {
    var a = 280
    var b = 560
    var c = a + b
    if c == 840 {
        print("PASS: comprehensive 280\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Comprehensive Test 280 ===\n")
    var failures = test_comprehensive_280()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
