fn test_is_even(x: int) -> int {
    return x % 2 == 0;
}

fn main() {
    var arr = [1, 2, 3, 4, 5, 2, 6];
    
    print("Testing 10 essential array methods:");
    
    // 1. wyn_array_find_index - Find index of first matching element
    var index = wyn_array_find_index(arr.data, arr.count, test_is_even);
    print("1. find_index(is_even) completed");
    
    // 2. wyn_array_reverse - Reverse array in place
    var rev_arr = [1, 2, 3, 4, 5];
    wyn_array_reverse(rev_arr.data, rev_arr.count);
    print("2. reverse completed");
    
    // 3. wyn_array_unique - Remove duplicates
    var unique_len = 0;
    var unique_arr = wyn_array_unique(arr.data, arr.count, &unique_len);
    print("3. unique completed");
    
    // 4. wyn_array_join - Join to string
    var joined = wyn_array_join(arr.data, arr.count, ",");
    print("4. join completed");
    
    // 5. wyn_array_slice - Get subarray
    var slice_len = 0;
    var slice_arr = wyn_array_slice(arr.data, 1, 4, &slice_len);
    print("5. slice completed");
    
    // 6. wyn_array_contains - Check if contains value
    var contains_3 = wyn_array_contains(arr.data, arr.count, 3);
    print("6. contains completed");
    
    // 7. wyn_array_first - Get first element
    var first_found = 0;
    var first_val = wyn_array_first(arr.data, arr.count, &first_found);
    print("7. first completed");
    
    // 8. wyn_array_last - Get last element
    var last_found = 0;
    var last_val = wyn_array_last(arr.data, arr.count, &last_found);
    print("8. last completed");
    
    // 9. wyn_array_is_empty - Check if empty
    var is_empty_arr = wyn_array_is_empty(arr.data, arr.count);
    print("9. is_empty completed");
    
    // 10. wyn_array_find - Find first matching element
    var found = 0;
    var result = wyn_array_find(arr.data, arr.count, test_is_even, &found);
    print("10. find completed");
    
    print("All 10 array methods tested successfully!");
    
    return 0;
}