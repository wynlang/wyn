// main.wyn - Main module demonstrating imports
import math.geometry.{Point, distance}
// Alternative import styles:
// import math.geometry.*
// import math.geometry as geo

fn main() {
    var p1 = Point{x: 0.0, y: 0.0}
    var p2 = Point{x: 3.0, y: 4.0}
    var dist = distance(p1, p2)  // Should be 5.0
    
    println("Distance between points: {}", dist)
}

// Example with aliased import
import math.geometry.{distance as calc_distance}

fn test_aliased() {
    var p1 = Point{x: 1.0, y: 1.0}
    var p2 = Point{x: 4.0, y: 5.0}
    var result = calc_distance(p1, p2)
    println("Aliased distance: {}", result)
}
