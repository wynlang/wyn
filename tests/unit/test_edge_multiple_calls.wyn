// Edge Case Test: Method chaining and complex scenarios
fn main() -> int {
    // Multiple consecutive method calls on same object
    var text = "Hello World";
    var c1 = text.contains("H");
    var c2 = text.contains("e");
    var c3 = text.contains("l");
    var c4 = text.contains("o");
    var c5 = text.contains("W");
    var c6 = text.contains("r");
    var c7 = text.contains("d");
    var c8 = text.contains("x");
    
    println("Multiple contains:");
    println(c1);
    println(c2);
    println(c3);
    println(c4);
    println(c5);
    println(c6);
    println(c7);
    println(c8);
    
    // Mixed method types
    var upper = text.upper();
    var lower = text.lower();
    var starts = text.starts_with("Hello");
    var ends = text.ends_with("World");
    var length = len(text);
    
    println("Mixed methods work:");
    println(starts);
    println(ends);
    println(length);
    
    // Multiple typeof calls
    var t1 = typeof(text);
    var t2 = typeof(c1);
    var t3 = typeof(length);
    println("Types:");
    println(t1);
    println(t2);
    println(t3);
    
    return 0;
}
