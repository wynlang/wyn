// Struct Example - Demonstrates structs and methods

struct Rectangle {
    width: int,
    height: int
}

fn rect_area(r: Rectangle) -> int {
    return r.width * r.height;
}

fn rect_perimeter(r: Rectangle) -> int {
    return 2 * (r.width + r.height);
}

fn main() -> int {
    var rect = Rectangle { width: 5, height: 10 };
    var area = rect_area(rect);
    var perim = rect_perimeter(rect);
    return area + perim;
}
