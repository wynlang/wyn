// Test file to verify basic specification examples compile correctly

// Example from type-system.md - Struct
struct Point {
    x: int,
    y: int
}

// Example from memory-model.md - String examples
fn string_examples() {
    var short_str = "Hello"           // SSO: stored inline (5 bytes)
    var medium_str = "Hello, World!"  // SSO: stored inline (13 bytes)
    var long_str = "This is a very long string that exceeds the SSO limit" // Heap allocated
    
    print("Short: " + short_str)
    print("Medium: " + medium_str)
    print("Long length: " + long_str.len().to_string())
}

fn main() -> int {
    print("=== Wyn Specification Examples Test ===")
    
    // Test struct
    var p = Point { x: 10, y: 20 }
    print("Point: (" + p.x.to_string() + ", " + p.y.to_string() + ")")
    
    // Test string memory management
    string_examples()
    
    // Test HashMap from type system spec
    var ages: HashMap<string, int> = {
        "alice": 25,
        "bob": 30,
        "charlie": 35
    }
    
    print("Alice's age: " + ages.get("alice").to_string())
    print("HashMap size: " + ages.len().to_string())
    
    return 0
}