// TDD Test: Regression test 459

fn test_regression_459() -> int {
    var x = 459
    var y = x * 2
    if y == 918 {
        print("PASS: regression 459\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 459 ===\n")
    var failures = test_regression_459()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
