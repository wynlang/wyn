// TDD Test: Regression test 427

fn test_regression_427() -> int {
    var x = 427
    var y = x * 2
    if y == 854 {
        print("PASS: regression 427\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 427 ===\n")
    var failures = test_regression_427()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
