// TDD: Array parameter passing and return values
// Tests passing arrays to functions and getting results back

fn sum_array(arr: [int], n: int) -> int {
    var total = 0
    for i in 0..n {
        total = total + arr[i]
    }
    return total
}

fn show_array(arr: [int], n: int) {
    var s = "["
    for i in 0..n {
        if i > 0 { s = s + ", " }
        s = s + int_to_string(arr[i])
    }
    println(s + "]")
}

fn main() -> int {
    var nums = [10, 20, 30, 40, 50]

    // Test 1: pass array to function
    var total = sum_array(nums, 5)
    println(total)

    // Test 2: pass array to void function
    show_array(nums, 5)

    return 0
}
