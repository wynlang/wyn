// Test: String collection methods
// Expected: Exit code 0

fn main() -> int {
    // Test split
    var csv = "a,b,c";
    var parts = csv.split(",");
    if (parts.len() != 3) {
        return 1;
    }
    
    // Test chars
    var text = "abc";
    var chars = text.chars();
    if (chars.len() != 3) {
        return 2;
    }
    
    // Test to_bytes
    var bytes = "AB".to_bytes();
    if (bytes.len() != 2) {
        return 3;
    }
    
    // Test lines
    var multiline = "line1\nline2\nline3";
    var lines = multiline.lines();
    if (lines.len() != 3) {
        return 4;
    }
    
    // Test words
    var sentence = "hello world test";
    var words = sentence.words();
    if (words.len() != 3) {
        return 5;
    }
    
    // Test pad_left
    var padded_left = "hi".pad_left(5, " ");
    if (padded_left.len() != 5) {
        return 6;
    }
    
    // Test pad_right
    var padded_right = "hi".pad_right(5, " ");
    if (padded_right.len() != 5) {
        return 7;
    }
    
    return 0;
}