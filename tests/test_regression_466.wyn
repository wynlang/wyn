// TDD Test: Regression test 466

fn test_regression_466() -> int {
    var x = 466
    var y = x * 2
    if y == 932 {
        print("PASS: regression 466\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 466 ===\n")
    var failures = test_regression_466()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
