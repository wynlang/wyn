// TDD Test: Regression test 457

fn test_regression_457() -> int {
    var x = 457
    var y = x * 2
    if y == 914 {
        print("PASS: regression 457\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 457 ===\n")
    var failures = test_regression_457()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
