fn is_leap_year(year: int) -> int {
    var div4 = year % 4;
    if div4 == 0 { return 1; }
    return 0;
}

fn main() -> int {
    var a = is_leap_year(2024);  // 1
    var b = is_leap_year(2020);  // 1
    var c = is_leap_year(2023);  // 0
    var d = is_leap_year(2021);  // 0
    return a + b + c + d;  // Should be 2
}
