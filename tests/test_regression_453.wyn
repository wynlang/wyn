// TDD Test: Regression test 453

fn test_regression_453() -> int {
    var x = 453
    var y = x * 2
    if y == 906 {
        print("PASS: regression 453\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 453 ===\n")
    var failures = test_regression_453()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
