// TDD Test: Regression test 497

fn test_regression_497() -> int {
    var x = 497
    var y = x * 2
    if y == 994 {
        print("PASS: regression 497\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 497 ===\n")
    var failures = test_regression_497()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
