// Test HashSet implementation with OO syntax
fn main() -> int {
    // Test creation with initialization
    var set = {:"apple", "banana", "cherry"};
    
    if (!hashset_contains(set, "apple")) {
        return 1;
    }
    
    if (!hashset_contains(set, "banana")) {
        return 2;
    }
    
    if (hashset_contains(set, "missing")) {
        return 3;
    }
    
    // Test add
    hashset_add(set, "date");
    if (!hashset_contains(set, "date")) {
        return 4;
    }
    
    // Test remove
    hashset_remove(set, "banana");
    if (hashset_contains(set, "banana")) {
        return 5;
    }
    
    return 0;
}
