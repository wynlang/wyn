// TDD Test: Regression test 412

fn test_regression_412() -> int {
    var x = 412
    var y = x * 2
    if y == 824 {
        print("PASS: regression 412\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 412 ===\n")
    var failures = test_regression_412()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
