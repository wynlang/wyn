// TDD Test: starts_with and ends_with methods
fn main() -> int {
    var text = "Hello World";
    
    var starts_hello = text.starts_with("Hello");
    var starts_world = text.starts_with("World");
    var ends_world = text.ends_with("World");
    var ends_hello = text.ends_with("Hello");
    
    println("starts_with('Hello'):");
    println(starts_hello);
    
    println("starts_with('World'):");
    println(starts_world);
    
    println("ends_with('World'):");
    println(ends_world);
    
    println("ends_with('Hello'):");
    println(ends_hello);
    
    if (starts_hello == 1 && starts_world == 0 && ends_world == 1 && ends_hello == 0) {
        println("✓ starts_with/ends_with work!");
        return 0;
    } else {
        println("✗ starts_with/ends_with failed");
        return 1;
    }
}
