// TDD Test: Comprehensive test 270

fn test_comprehensive_270() -> int {
    var a = 270
    var b = 540
    var c = a + b
    if c == 810 {
        print("PASS: comprehensive 270\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Comprehensive Test 270 ===\n")
    var failures = test_comprehensive_270()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
