// TDD Test: Regression test 431

fn test_regression_431() -> int {
    var x = 431
    var y = x * 2
    if y == 862 {
        print("PASS: regression 431\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 431 ===\n")
    var failures = test_regression_431()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
