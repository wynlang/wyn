// TDD Test: Operator precedence

fn test_multiply_before_add() -> int {
    var result = 2 + 3 * 4
    if result == 14 {
        print("PASS: multiply before add\n")
        return 0
    }
    return 1
}

fn test_divide_before_subtract() -> int {
    var result = 10 - 8 / 2
    if result == 6 {
        print("PASS: divide before subtract\n")
        return 0
    }
    return 1
}

fn test_parentheses_override() -> int {
    var result = (2 + 3) * 4
    if result == 20 {
        print("PASS: parentheses override\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Precedence Tests ===\n")
    var failures = 0
    failures = failures + test_multiply_before_add()
    failures = failures + test_divide_before_subtract()
    failures = failures + test_parentheses_override()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
