// Task 4.4: Float Methods - Simple test
fn main() -> int {
    // Test with positive float
    var x = 3.7
    
    // Test floor
    var f = x.floor()
    if f != 3.0 { return 1; }
    
    // Test ceil
    var c = x.ceil()
    if c != 4.0 { return 2; }
    
    // Test round
    var r = x.round()
    if r != 4.0 { return 3; }
    
    return 0
}
