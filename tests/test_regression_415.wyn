// TDD Test: Regression test 415

fn test_regression_415() -> int {
    var x = 415
    var y = x * 2
    if y == 830 {
        print("PASS: regression 415\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 415 ===\n")
    var failures = test_regression_415()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
