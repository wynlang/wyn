// TDD Test: Operator precedence

fn test_add_multiply() -> int {
    if 2 + 3 * 4 == 14 {
        print("PASS: add multiply\n")
        return 0
    }
    return 1
}

fn test_subtract_divide() -> int {
    if 10 - 6 / 2 == 7 {
        print("PASS: subtract divide\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Operator Tests ===\n")
    var failures = 0
    failures = failures + test_add_multiply()
    failures = failures + test_subtract_divide()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
