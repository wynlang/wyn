// TDD Test: String operations

fn test_string_concat() -> int {
    var s1: string = "hello"
    var s2: string = " world"
    var result = s1 + s2
    var expected: string = "hello world"
    if result == expected {
        print("PASS: string concat\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== String Multiline Tests ===\n")
    var failures = test_string_concat()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
