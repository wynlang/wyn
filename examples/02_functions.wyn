// Functions - Defining and calling functions
fn add(a: int, b: int) -> int {
    return a + b;
}

fn multiply(a: int, b: int) -> int {
    return a * b;
}

fn main() -> int {
    var sum = add(5, 3);
    var product = multiply(4, 7);
    
    print_str("Sum: ");
    print(sum);
    print_str("Product: ");
    print(product);
    
    return 0;
}
