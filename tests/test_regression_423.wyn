// TDD Test: Regression test 423

fn test_regression_423() -> int {
    var x = 423
    var y = x * 2
    if y == 846 {
        print("PASS: regression 423\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 423 ===\n")
    var failures = test_regression_423()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
