// Edge Case Test: Repeated operations (memory safety)
fn main() -> int {
    var text = "Hello World";
    
    // Many repeated contains calls
    var r1 = text.contains("H");
    var r2 = text.contains("e");
    var r3 = text.contains("l");
    var r4 = text.contains("l");
    var r5 = text.contains("o");
    var r6 = text.contains(" ");
    var r7 = text.contains("W");
    var r8 = text.contains("o");
    var r9 = text.contains("r");
    var r10 = text.contains("l");
    var r11 = text.contains("d");
    var r12 = text.contains("x");
    var r13 = text.contains("y");
    var r14 = text.contains("z");
    var r15 = text.contains("Hello");
    
    println("15 contains calls completed");
    
    // Many repeated upper/lower calls
    var u1 = text.upper();
    var l1 = text.lower();
    var u2 = text.upper();
    var l2 = text.lower();
    var u3 = text.upper();
    var l3 = text.lower();
    
    println("6 upper/lower calls completed");
    
    // Many repeated starts/ends calls
    var s1 = text.starts_with("H");
    var e1 = text.ends_with("d");
    var s2 = text.starts_with("He");
    var e2 = text.ends_with("ld");
    var s3 = text.starts_with("Hel");
    var e3 = text.ends_with("rld");
    
    println("6 starts/ends calls completed");
    
    // Many repeated len calls
    var len1 = len(text);
    var len2 = len(text);
    var len3 = len(text);
    var len4 = len(text);
    var len5 = len(text);
    
    println("5 len calls completed");
    
    // Many repeated typeof calls
    var t1 = typeof(text);
    var t2 = typeof(text);
    var t3 = typeof(text);
    var t4 = typeof(text);
    var t5 = typeof(text);
    
    println("5 typeof calls completed");
    
    println("All repeated operations successful!");
    return 0;
}
