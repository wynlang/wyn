// Fibonacci - Demonstrates recursion and performance

fn fibonacci(n: int) -> int {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

fn main() -> int {
    for i in 0..10 {
        println("fib(" + i.to_string() + ") = " + fibonacci(i).to_string())
    }
    return 0
}
