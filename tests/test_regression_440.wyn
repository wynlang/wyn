// TDD Test: Regression test 440

fn test_regression_440() -> int {
    var x = 440
    var y = x * 2
    if y == 880 {
        print("PASS: regression 440\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 440 ===\n")
    var failures = test_regression_440()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
