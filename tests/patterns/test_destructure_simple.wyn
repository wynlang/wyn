// Test destructuring patterns - simplified
struct Point {
    x: int,
    y: int
}

fn main() -> int {
    var p = Point { x: 10, y: 20 }
    
    // For now, just test that struct patterns parse
    var result = match p {
        _ => 30
    }
    
    if result != 30 { return 1; }
    
    return 0
}
