// TDD: Test float negation and abs()
fn main() -> int {
    // Test 1: Negative float literal
    var x = -3.14
    if x >= 0.0 { return 1; }
    
    // Test 2: Negation of variable
    var y = 2.5
    var z = -y
    if z >= 0.0 { return 2; }
    
    // Test 3: Float abs() with negative
    var a = -5.5
    var b = a.abs()
    if b < 5.4 { return 3; }
    if b > 5.6 { return 3; }
    
    // Test 4: Float abs() with positive
    var c = 7.7
    var d = c.abs()
    if d < 7.6 { return 4; }
    if d > 7.8 { return 4; }
    
    return 0
}
