// TDD Test: Regression test 471

fn test_regression_471() -> int {
    var x = 471
    var y = x * 2
    if y == 942 {
        print("PASS: regression 471\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 471 ===\n")
    var failures = test_regression_471()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
