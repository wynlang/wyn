// Comprehensive demo of enhanced Wyn features
fn main() -> int {
    println("=== Wyn Language Enhanced Features Demo ===");
    println("");
    
    // Print functions
    println("1. Print Functions:");
    print("   - print() and println() work!");
    println("");
    
    // Math functions
    println("2. Math Functions:");
    var a = 15;
    var b = 25;
    print("   min(15, 25) = ");
    println(min(a, b));
    print("   max(15, 25) = ");
    println(max(a, b));
    print("   abs(-42) = ");
    println(abs(-42));
    
    // Option/Result types
    println("");
    println("3. Option/Result Types:");
    var opt = some(100);
    var nothing = none;
    var success = ok(200);
    var failure = err(404);
    println("   Option and Result types working!");
    
    // File I/O
    println("");
    println("4. File I/O:");
    var write_result = file_write("/tmp/wyn_demo.txt", "Hello from Wyn!");
    if write_result == 1 {
        println("   File written successfully!");
    }
    
    var exists = file_exists("/tmp/wyn_demo.txt");
    if exists == 1 {
        println("   File exists!");
    }
    
    println("");
    println("=== All features working! ===");
    return 0;
}
