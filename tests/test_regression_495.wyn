// TDD Test: Regression test 495

fn test_regression_495() -> int {
    var x = 495
    var y = x * 2
    if y == 990 {
        print("PASS: regression 495\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 495 ===\n")
    var failures = test_regression_495()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
