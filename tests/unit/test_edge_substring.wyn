// Edge Case Test: Substring matching
fn main() -> int {
    var text = "Hello World";
    
    // Full string match
    var full_match = text.contains("Hello World");
    println("Contains full string:");
    println(full_match);
    
    // Single character
    var single = text.contains("H");
    println("Contains single char:");
    println(single);
    
    // Last character
    var last = text.contains("d");
    println("Contains last char:");
    println(last);
    
    // Not found
    var not_found = text.contains("xyz");
    println("Contains 'xyz':");
    println(not_found);
    
    // Case sensitive
    var lower_h = text.contains("h");
    println("Contains lowercase 'h':");
    println(lower_h);
    
    // Starts with full string
    var starts_full = text.starts_with("Hello World");
    println("Starts with full:");
    println(starts_full);
    
    // Starts with single char
    var starts_single = text.starts_with("H");
    println("Starts with 'H':");
    println(starts_single);
    
    // Ends with full string
    var ends_full = text.ends_with("Hello World");
    println("Ends with full:");
    println(ends_full);
    
    // Ends with single char
    var ends_single = text.ends_with("d");
    println("Ends with 'd':");
    println(ends_single);
    
    // Doesn't start with
    var not_starts = text.starts_with("World");
    println("Starts with 'World':");
    println(not_starts);
    
    // Doesn't end with
    var not_ends = text.ends_with("Hello");
    println("Ends with 'Hello':");
    println(not_ends);
    
    return 0;
}
