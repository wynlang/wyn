// Test HashSet with OO method syntax
fn main() -> int {
    // Test creation with initialization
    var set = {:"apple", "banana", "cherry"};
    
    if (!set.contains("apple")) {
        return 1;
    }
    
    if (!set.contains("banana")) {
        return 2;
    }
    
    if (set.contains("missing")) {
        return 3;
    }
    
    // Test insert method
    set.insert("date");
    if (!set.contains("date")) {
        return 4;
    }
    
    // Test remove method
    set.remove("banana");
    if (set.contains("banana")) {
        return 5;
    }
    
    return 0;
}
