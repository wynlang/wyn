// TDD Test: Quality test 175

fn test_operation_175() -> int {
    var a = 175
    var b = 185
    var sum = a + b
    if sum == 360 {
        print("PASS: operation 175\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Quality Test 175 ===\n")
    var failures = test_operation_175()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
