// TDD Test: Regression test 450

fn test_regression_450() -> int {
    var x = 450
    var y = x * 2
    if y == 900 {
        print("PASS: regression 450\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 450 ===\n")
    var failures = test_regression_450()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
