// Test: Array functional methods (Part 1)
// Expected: Exit code 0

fn main() -> int {
    // Create test array
    let arr = [1, 2, 3, 4, 5];
    
    // Test take() method
    let taken = arr.take(3);
    if (taken.len() != 3) {
        return 1;
    }
    
    // Test skip() method
    let skipped = arr.skip(2);
    if (skipped.len() != 3) {
        return 2;
    }
    
    // Test slice() method
    let sliced = arr.slice(1, 4);
    if (sliced.len() != 3) {
        return 3;
    }
    
    // Test concat() method
    let arr2 = [6, 7, 8];
    let concatenated = arr.concat(arr2);
    if (concatenated.len() != 8) {
        return 4;
    }
    
    return 0;
}