// TDD Test: Regression test 445

fn test_regression_445() -> int {
    var x = 445
    var y = x * 2
    if y == 890 {
        print("PASS: regression 445\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 445 ===\n")
    var failures = test_regression_445()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
