// TDD Test: Regression test 464

fn test_regression_464() -> int {
    var x = 464
    var y = x * 2
    if y == 928 {
        print("PASS: regression 464\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 464 ===\n")
    var failures = test_regression_464()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
