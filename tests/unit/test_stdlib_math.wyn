// Math stdlib functions
fn abs_test(x: int) -> int {
    if x < 0 { return 0 - x; }
    return x;
}

fn max_test(a: int, b: int) -> int {
    if a > b { return a; }
    return b;
}

fn wyn_pow(x: int, n: int) -> int {
    var result = 1;
    var i = 0;
    while i < n {
        result = result * x;
        i = i + 1;
    }
    return result;
}

fn main() -> int {
    var a = abs_test(0 - 5);      // 5
    var m = max_test(10, 20);      // 20
    var p = wyn_pow(2, 3);        // 8
    return a + m + p;             // 5 + 20 + 8 = 33
}
