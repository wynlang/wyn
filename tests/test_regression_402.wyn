// TDD Test: Regression test 402

fn test_regression_402() -> int {
    var x = 402
    var y = x * 2
    if y == 804 {
        print("PASS: regression 402\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 402 ===\n")
    var failures = test_regression_402()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
