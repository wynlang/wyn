fn main() -> int {
    var area = 10 * 20;
    var values = [100, 200, area];
    var selected = values[2];
    
    var result = match selected {
        200 => 1,
        _ => 2
    };
    
    var final_value = result;
    
    match final_value {
        2 => return 42,
        1 => return 99,
        _ => return 0
    }
}
