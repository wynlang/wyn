// TDD Test: Regression test 476

fn test_regression_476() -> int {
    var x = 476
    var y = x * 2
    if y == 952 {
        print("PASS: regression 476\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 476 ===\n")
    var failures = test_regression_476()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
