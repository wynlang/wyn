fn add(a: int, b: int) -> int {
    return a + b;
}

fn multiply(a: int, b: int, c: int) -> int {
    return a * b * c;
}

fn main() -> int {
    var f1 = spawn add(10, 20);
    var f2 = spawn multiply(2, 3, 4);
    
    var r1 = f1.await();
    var r2 = f2.await();
    
    if r1 == 30 && r2 == 24 {
        print(1);
    }
    return 0;
}
