// TDD Test: Regression test 475

fn test_regression_475() -> int {
    var x = 475
    var y = x * 2
    if y == 950 {
        print("PASS: regression 475\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 475 ===\n")
    var failures = test_regression_475()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
