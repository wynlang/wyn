// TDD Test: Final test 148

fn test_comprehensive_operations() -> int {
    var x = 10
    var y = 20
    var sum = x + y
    var product = x * y
    
    if sum == 30 {
        if product == 200 {
            print("PASS: comprehensive operations\n")
            return 0
        }
    }
    return 1
}

fn main() -> int {
    print("=== Final Test 148 ===\n")
    var failures = test_comprehensive_operations()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
