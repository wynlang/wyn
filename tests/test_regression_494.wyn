// TDD Test: Regression test 494

fn test_regression_494() -> int {
    var x = 494
    var y = x * 2
    if y == 988 {
        print("PASS: regression 494\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 494 ===\n")
    var failures = test_regression_494()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
