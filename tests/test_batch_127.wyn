// TDD Test: Batch test 127

fn test_batch_127() -> int {
    var x = 127
    var y = 127 * 2
    if y == 254 {
        print("PASS: batch 127\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Batch Test 127 ===\n")
    var failures = test_batch_127()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
