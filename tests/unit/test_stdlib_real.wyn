// Test REAL stdlib modules

// Math functions
fn abs_stdlib(x: int) -> int {
    if x < 0 { return 0 - x; }
    return x;
}

fn max_stdlib(a: int, b: int) -> int {
    if a > b { return a; }
    return b;
}

fn wyn_pow(x: int, n: int) -> int {
    var result = 1;
    var i = 0;
    while i < n {
        result = result * x;
        i = i + 1;
    }
    return result;
}

// Array functions
fn array_sum_stdlib(arr: [int], len: int) -> int {
    var total = 0;
    var i = 0;
    while i < len {
        total = total + arr[i];
        i = i + 1;
    }
    return total;
}

fn main() -> int {
    // Test math
    var a = abs_stdlib(0 - 5);
    var m = max_stdlib(10, 20);
    var p = wyn_pow(2, 3);
    
    // Test arrays
    var arr = [1, 2, 3, 4, 5];
    var s = array_sum_stdlib(arr, 5);
    
    // Return combined result
    return a + m + p + s;
}
