// Structs - Defining and using structures
struct Point {
    x: int,
    y: int
}

fn distance_from_origin(p: Point) -> int {
    return p.x * p.x + p.y * p.y;
}

fn main() -> int {
    var p1 = Point { x: 3, y: 4 };
    var p2 = Point { x: 5, y: 12 };
    
    var dist1 = distance_from_origin(p1);
    var dist2 = distance_from_origin(p2);
    
    print_str("Distance 1: ");
    print(dist1);
    print_str("Distance 2: ");
    print(dist2);
    
    return 0;
}
