// TDD Test: time_now API
fn main() -> int {
    println("Testing time_now...");
    
    var t1 = time_now();
    sleep(100);
    var t2 = time_now();
    
    println("Time 1:");
    println(t1);
    println("Time 2:");
    println(t2);
    
    if (t2 > t1) {
        println("✓ time_now works!");
        return 0;
    } else {
        println("✗ time_now failed");
        return 1;
    }
}
