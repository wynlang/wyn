// TDD Test: Regression test 437

fn test_regression_437() -> int {
    var x = 437
    var y = x * 2
    if y == 874 {
        print("PASS: regression 437\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 437 ===\n")
    var failures = test_regression_437()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
