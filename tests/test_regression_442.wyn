// TDD Test: Regression test 442

fn test_regression_442() -> int {
    var x = 442
    var y = x * 2
    if y == 884 {
        print("PASS: regression 442\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 442 ===\n")
    var failures = test_regression_442()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
