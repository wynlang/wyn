// Test: Missing pattern arm should produce compilation error
enum Status {
    Active,
    Inactive,
    Pending,
    Suspended
}

fn main() -> int {
    var status = Active
    var result = 0
    
    // Error: Missing Suspended case - should fail compilation
    match status {
        Active => result = 1,
        Inactive => result = 2,
        Pending => result = 3
        // Missing: Suspended => result = 4
    }
    
    print_str("This should not compile due to missing pattern arm")
    return result
}