// TDD Test: Regression test 441

fn test_regression_441() -> int {
    var x = 441
    var y = x * 2
    if y == 882 {
        print("PASS: regression 441\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 441 ===\n")
    var failures = test_regression_441()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
