// TDD: Basic regex/pattern matching operations

fn main() -> int {
    // Test 1: Regex.match - check if string matches pattern
    println(Regex.match("hello123", "[a-z]+[0-9]+"))
    println(Regex.match("hello", "[0-9]+"))

    // Test 2: Regex.replace - replace pattern with string
    var result = Regex.replace("hello world", "world", "Wyn")
    println(result)

    return 0
}
