fn compute(n: int) -> int {
    return n * n;
}

fn main() -> int {
    // Test 1: Capture Future from spawn
    var f = spawn compute(42);
    
    // Test 2: Call .await() to get result
    var result = f.await();
    
    // Test 3: Verify result
    if result == 1764 {
        print(1);
    }
    
    return 0;
}
