// TDD Test: Regression test 461

fn test_regression_461() -> int {
    var x = 461
    var y = x * 2
    if y == 922 {
        print("PASS: regression 461\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 461 ===\n")
    var failures = test_regression_461()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
