// TDD Test: Two method calls
fn main() -> int {
    println("Test: Two method calls");
    
    var text = "Hello World";
    
    var has_hello = text.contains("Hello");
    println(has_hello);
    
    var has_world = text.contains("World");
    println(has_world);
    
    println("Done!");
    return 0;
}
