// TDD Test: Regression test 474

fn test_regression_474() -> int {
    var x = 474
    var y = x * 2
    if y == 948 {
        print("PASS: regression 474\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 474 ===\n")
    var failures = test_regression_474()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
