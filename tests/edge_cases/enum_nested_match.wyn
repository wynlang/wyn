// Test: Match statements with enums
enum Status {
    Active,
    Inactive,
    Pending
}

enum Priority {
    High,
    Medium,
    Low
}

fn main() -> int {
    var status = Active
    var priority = High
    
    var status_code = 0
    var priority_code = 0
    
    match status {
        Active => status_code = 1,
        Inactive => status_code = 2,
        Pending => status_code = 3
    }
    
    match priority {
        High => priority_code = 10,
        Medium => priority_code = 20,
        Low => priority_code = 30
    }
    
    if status_code == 1 && priority_code == 10 {
        print_str("Match statements work correctly")
        return 0
    } else {
        print_str("Match statements failed")
        return 1
    }
}