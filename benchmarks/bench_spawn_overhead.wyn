// Benchmark: Spawn overhead (10,000 spawns)
fn noop() -> int {
    return 0;
}

fn main() -> int {
    var start = time_now();
    
    var i = 0;
    while i < 10000 {
        spawn noop();
        i = i + 1;
    }
    
    var elapsed = time_now() - start;
    print(elapsed);  // Print nanoseconds
    return 0;
}
