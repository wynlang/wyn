// TDD Test: Regression test 435

fn test_regression_435() -> int {
    var x = 435
    var y = x * 2
    if y == 870 {
        print("PASS: regression 435\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 435 ===\n")
    var failures = test_regression_435()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
