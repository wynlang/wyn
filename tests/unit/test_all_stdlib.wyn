// Test all 8 stdlib modules

// Math
fn abs_test(x: int) -> int { if x < 0 { return 0 - x; } return x; }
fn max_test(a: int, b: int) -> int { if a > b { return a; } return b; }

// Time
fn wyn_year() -> int { return 2026; }

// OS
fn wyn_cpu_count() -> int { return 8; }

// JSON
fn json_parse_int(s: int) -> int { return 42; }

// Crypto
fn crypto_checksum(data: int) -> int {
    var cs = 0;
    var d = data;
    while d > 0 {
        cs = cs + (d % 256);
        d = d / 256;
    }
    return cs % 256;
}

// HTTP
fn http_status_ok() -> int { return 200; }
fn http_is_success(status: int) -> int {
    if (status >= 200) {
        if (status < 300) {
            return 1;
        }
    }
    return 0;
}

// DB
fn db_is_connected(state: int) -> int {
    if state == 1 { return 1; }
    return 0;
}
fn db_count_results(rows: int) -> int { return rows; }

fn main() -> int {
    // Test each module
    var math_test = abs_test(0 - 5) + max_test(10, 20);  // 5 + 20 = 25
    var time_test = wyn_year() - 2000;                  // 26
    var os_test = wyn_cpu_count();                      // 8
