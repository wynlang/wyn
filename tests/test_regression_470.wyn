// TDD Test: Regression test 470

fn test_regression_470() -> int {
    var x = 470
    var y = x * 2
    if y == 940 {
        print("PASS: regression 470\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 470 ===\n")
    var failures = test_regression_470()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
