// Test trait implementation
trait Display {
    fn show(self) -> int;
}

struct Point {
    x: int,
    y: int
}

impl Display for Point {
    fn show(self) -> int {
        return self.x + self.y;
    }
}

fn main() -> int {
    let p = Point { x: 10, y: 20 };
    return p.show();  // Should return 30
}
