// TDD Test: Regression test 408

fn test_regression_408() -> int {
    var x = 408
    var y = x * 2
    if y == 816 {
        print("PASS: regression 408\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 408 ===\n")
    var failures = test_regression_408()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
