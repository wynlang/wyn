// TDD Test: Regression test 458

fn test_regression_458() -> int {
    var x = 458
    var y = x * 2
    if y == 916 {
        print("PASS: regression 458\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 458 ===\n")
    var failures = test_regression_458()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
