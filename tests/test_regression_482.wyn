// TDD Test: Regression test 482

fn test_regression_482() -> int {
    var x = 482
    var y = x * 2
    if y == 964 {
        print("PASS: regression 482\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 482 ===\n")
    var failures = test_regression_482()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
