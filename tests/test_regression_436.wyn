// TDD Test: Regression test 436

fn test_regression_436() -> int {
    var x = 436
    var y = x * 2
    if y == 872 {
        print("PASS: regression 436\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 436 ===\n")
    var failures = test_regression_436()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
