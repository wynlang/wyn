// TDD Test: Regression test 414

fn test_regression_414() -> int {
    var x = 414
    var y = x * 2
    if y == 828 {
        print("PASS: regression 414\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 414 ===\n")
    var failures = test_regression_414()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
