// TDD Test: Regression test 424

fn test_regression_424() -> int {
    var x = 424
    var y = x * 2
    if y == 848 {
        print("PASS: regression 424\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 424 ===\n")
    var failures = test_regression_424()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
