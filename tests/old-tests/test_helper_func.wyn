enum TokenType {
    EndOfFile,
    Int
}

struct Token {
    tok_type: TokenType,
    value: string
}

struct Parser {
    tokens: [Token],
    current: int
}

fn make_token(tok_type: TokenType, value: string) -> Token {
    return Token { tok_type: tok_type, value: value }
}

fn parser_peek(mut p: Parser) -> Token {
    var len = p.tokens.len()
    return make_token(TokenType_EndOfFile, "")
}

fn main() -> int {
    var tokens: [Token] = []
    var p = Parser { tokens: tokens, current: 0 }
    var tok = parser_peek(p)
    return 0
}
