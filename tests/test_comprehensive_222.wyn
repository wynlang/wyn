// TDD Test: Comprehensive test 222

fn test_comprehensive_222() -> int {
    var a = 222
    var b = 444
    var c = a + b
    if c == 666 {
        print("PASS: comprehensive 222\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Comprehensive Test 222 ===\n")
    var failures = test_comprehensive_222()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
