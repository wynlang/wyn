// TDD Test: Regression test 420

fn test_regression_420() -> int {
    var x = 420
    var y = x * 2
    if y == 840 {
        print("PASS: regression 420\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 420 ===\n")
    var failures = test_regression_420()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
