// TDD Test: Regression test 438

fn test_regression_438() -> int {
    var x = 438
    var y = x * 2
    if y == 876 {
        print("PASS: regression 438\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 438 ===\n")
    var failures = test_regression_438()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
