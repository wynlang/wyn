// TDD Test: Regression test 491

fn test_regression_491() -> int {
    var x = 491
    var y = x * 2
    if y == 982 {
        print("PASS: regression 491\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 491 ===\n")
    var failures = test_regression_491()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
