// TDD Test: Comprehensive test 250

fn test_comprehensive_250() -> int {
    var a = 250
    var b = 500
    var c = a + b
    if c == 750 {
        print("PASS: comprehensive 250\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Comprehensive Test 250 ===\n")
    var failures = test_comprehensive_250()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
