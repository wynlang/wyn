// TDD Test: Regression test 404

fn test_regression_404() -> int {
    var x = 404
    var y = x * 2
    if y == 808 {
        print("PASS: regression 404\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 404 ===\n")
    var failures = test_regression_404()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
