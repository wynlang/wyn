// TDD Test: Regression test 485

fn test_regression_485() -> int {
    var x = 485
    var y = x * 2
    if y == 970 {
        print("PASS: regression 485\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 485 ===\n")
    var failures = test_regression_485()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
