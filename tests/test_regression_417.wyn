// TDD Test: Regression test 417

fn test_regression_417() -> int {
    var x = 417
    var y = x * 2
    if y == 834 {
        print("PASS: regression 417\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 417 ===\n")
    var failures = test_regression_417()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
