// TDD: String stdlib expansion

fn main() -> int {
    // Test .substring()
    var s = "Hello World"
    println(s.substring(0, 5))
    println(s.substring(6, 11))

    // Test .index_of()
    println(s.index_of("World"))
    println(s.index_of("xyz"))

    // Test .repeat()
    println("ab".repeat(3))

    // Test .starts_with() / .ends_with()
    println(s.starts_with("Hello"))
    println(s.ends_with("World"))
    println(s.starts_with("World"))

    return 0
}
