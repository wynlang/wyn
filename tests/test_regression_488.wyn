// TDD Test: Regression test 488

fn test_regression_488() -> int {
    var x = 488
    var y = x * 2
    if y == 976 {
        print("PASS: regression 488\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 488 ===\n")
    var failures = test_regression_488()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
