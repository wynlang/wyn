fn test_array_allocation() -> int {
    var arr = [int; 1000];
    
    for i in 0..1000 {
        arr[i] = i * 2;
    }
    
    var sum = 0;
    for i in 0..1000 {
        sum = sum + arr[i];
    }
    
    return sum;
}

fn main() -> int {
    var result = test_array_allocation();
    return 0;
}