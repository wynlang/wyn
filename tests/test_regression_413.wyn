// TDD Test: Regression test 413

fn test_regression_413() -> int {
    var x = 413
    var y = x * 2
    if y == 826 {
        print("PASS: regression 413\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 413 ===\n")
    var failures = test_regression_413()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
