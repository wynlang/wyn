// Test range patterns
fn main() -> int {
    var x = 5
    
    // Should match 0..10
    var result = match x {
        0..10 => 100,
        _ => 0
    }
    
    if result != 100 { return 1; }
    
    var y = 15
    var result2 = match y {
        0..10 => 100,
        _ => 0
    }
    
    if result2 != 0 { return 2; }
    
    return 0
}
