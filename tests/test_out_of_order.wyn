fn compute(n: int) -> int {
    return n * n;
}

fn main() -> int {
    var f1 = spawn compute(1);
    var f2 = spawn compute(2);
    var f3 = spawn compute(3);
    
    // Await in reverse order
    var r3 = f3.await();
    var r2 = f2.await();
    var r1 = f1.await();
    
    if r1 == 1 && r2 == 4 && r3 == 9 {
        print(1);
    }
    return 0;
}
