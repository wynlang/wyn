fn abs_test(x: int) -> int {
    if x < 0 { return 0 - x; }
    return x;
}

fn max_test(a: int, b: int) -> int {
    if a > b { return a; }
    return b;
}

fn wyn_pow(x: int, n: int) -> int {
    var result = 1;
    var i = 0;
    while i < n {
        result = result * x;
        i = i + 1;
    }
    return result;
}

fn main() -> int {
    // Test with different inputs
    var a1 = abs_test(0 - 15);      // 15
    var a2 = abs_test(20);           // 20
    var m1 = max_test(100, 50);      // 100
    var m2 = max_test(3, 7);         // 7
    var p1 = wyn_pow(3, 2);         // 9
    var p2 = wyn_pow(2, 4);         // 16
    
    // Should return 15+20+100+7+9+16 = 167
    return a1 + a2 + m1 + m2 + p1 + p2;
}
