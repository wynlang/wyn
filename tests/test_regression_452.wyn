// TDD Test: Regression test 452

fn test_regression_452() -> int {
    var x = 452
    var y = x * 2
    if y == 904 {
        print("PASS: regression 452\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 452 ===\n")
    var failures = test_regression_452()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
