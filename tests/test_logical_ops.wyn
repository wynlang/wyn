// TDD Test: Logical operations

fn test_and_true() -> int {
    if 1 == 1 {
        if 2 == 2 {
            print("PASS: and true\n")
            return 0
        }
    }
    print("FAIL: and true\n")
    return 1
}

fn test_and_false() -> int {
    var result = 1
    if 1 == 1 {
        if 2 == 3 {
            result = 0
        }
    }
    if result == 1 {
        print("PASS: and false\n")
        return 0
    }
    print("FAIL: and false\n")
    return 1
}

fn main() -> int {
    print("=== Logical Operations Tests ===\n")
    var failures = 0
    failures = failures + test_and_true()
    failures = failures + test_and_false()
    
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
