// TDD Test: Regression test 406

fn test_regression_406() -> int {
    var x = 406
    var y = x * 2
    if y == 812 {
        print("PASS: regression 406\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 406 ===\n")
    var failures = test_regression_406()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
