// Edge Case Test: typeof with all types
fn main() -> int {
    // Basic types
    var num = 42;
    var text = "hello";
    var zero = 0;
    var negative = 0 - 10;
    
    var t1 = typeof(num);
    var t2 = typeof(text);
    var t3 = typeof(zero);
    var t4 = typeof(negative);
    
    println("Type of 42:");
    println(t1);
    println("Type of string:");
    println(t2);
    println("Type of 0:");
    println(t3);
    println("Type of -10:");
    println(t4);
    
    // Typeof on results
    var concat = str_concat("a", "b");
    var upper = text.upper();
    var to_str = num.to_string();
    
    var t5 = typeof(concat);
    var t6 = typeof(upper);
    var t7 = typeof(to_str);
    
    println("Type of concat result:");
    println(t5);
    println("Type of upper result:");
    println(t6);
    println("Type of to_string result:");
    println(t7);
    
    // Typeof on len result
    var length = len(text);
    var t8 = typeof(length);
    println("Type of len result:");
    println(t8);
    
    // Typeof on method results
    var contains_result = text.contains("h");
    var t9 = typeof(contains_result);
    println("Type of contains result:");
    println(t9);
    
    return 0;
}
