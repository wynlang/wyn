// TDD Test: Quality test 170

fn test_operation_170() -> int {
    var a = 170
    var b = 180
    var sum = a + b
    if sum == 350 {
        print("PASS: operation 170\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Quality Test 170 ===\n")
    var failures = test_operation_170()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
