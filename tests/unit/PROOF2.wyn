struct Point { x: int, y: int }
fn add(a: int, b: int) -> int { return a + b; }
fn identity<T>(x: T) -> T { return x; }
trait Display { fn show() -> int; }

fn main() -> int {
    var p = Point { x: 5, y: 10 };
    var sum = add(p.x, p.y);
    var arr = [10, 20, 30];
    var result = match sum {
        15 => arr[2],
        _ => 0
    };
    return result;
}
