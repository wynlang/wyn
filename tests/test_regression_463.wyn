// TDD Test: Regression test 463

fn test_regression_463() -> int {
    var x = 463
    var y = x * 2
    if y == 926 {
        print("PASS: regression 463\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 463 ===\n")
    var failures = test_regression_463()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
