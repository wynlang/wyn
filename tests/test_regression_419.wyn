// TDD Test: Regression test 419

fn test_regression_419() -> int {
    var x = 419
    var y = x * 2
    if y == 838 {
        print("PASS: regression 419\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 419 ===\n")
    var failures = test_regression_419()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
