// TDD Test: Function return values

fn get_five() -> int {
    return 5
}

fn get_ten() -> int {
    return 10
}

fn test_return_five() -> int {
    var result = get_five()
    if result == 5 {
        print("PASS: return five\n")
        return 0
    }
    return 1
}

fn test_return_ten() -> int {
    var result = get_ten()
    if result == 10 {
        print("PASS: return ten\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Function Return Values Tests ===\n")
    var failures = 0
    failures = failures + test_return_five()
    failures = failures + test_return_ten()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
