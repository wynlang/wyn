// TDD Test: Comprehensive test 290

fn test_comprehensive_290() -> int {
    var a = 290
    var b = 580
    var c = a + b
    if c == 870 {
        print("PASS: comprehensive 290\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Comprehensive Test 290 ===\n")
