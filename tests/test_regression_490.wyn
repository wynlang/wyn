// TDD Test: Regression test 490

fn test_regression_490() -> int {
    var x = 490
    var y = x * 2
    if y == 980 {
        print("PASS: regression 490\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 490 ===\n")
    var failures = test_regression_490()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
