// TDD Test: Print operations

fn test_print_string() -> int {
    print("test string\n")
    print("PASS: print string\n")
    return 0
}

fn test_print_number() -> int {
    var n = 42
    print("PASS: print number\n")
    return 0
}

fn test_print_multiple() -> int {
    print("line 1\n")
    print("line 2\n")
    print("PASS: print multiple\n")
    return 0
}

fn main() -> int {
    print("=== Print Operations Tests ===\n")
    var failures = 0
    failures = failures + test_print_string()
    failures = failures + test_print_number()
    failures = failures + test_print_multiple()
    
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
