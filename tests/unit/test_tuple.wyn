fn main() -> int {
    // Test basic tuple creation
    let pair = (10, 20);
    let x = pair.0;
    let y = pair.1;
    
    // Test tuple with 3 elements
    let triple = (1, 2, 3);
    let a = triple.0;
    let b = triple.1;
    let c = triple.2;
    
    // Test tuple with 4 elements
    let quad = (5, 6, 7, 8);
    let p = quad.0;
    let q = quad.1;
    let r = quad.2;
    let s = quad.3;
    
    // Return sum of all elements
    return x + y + a + b + c + p + q + r + s;
    // 10 + 20 + 1 + 2 + 3 + 5 + 6 + 7 + 8 = 62
}

