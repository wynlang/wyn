// Test HashMap implementation
fn main() -> int {
    // Test basic insert and get
    let hm = hashmap_new();
    hashmap_insert(hm, "key1", 42);
    hashmap_insert(hm, "key2", 100);
    
    let val1 = hashmap_get(hm, "key1");
    let val2 = hashmap_get(hm, "key2");
    
    if val1 != 42 {
        return 1;
    }
    
    if val2 != 100 {
        return 2;
    }
    
    // Test overwrite
    hashmap_insert(hm, "key1", 99);
    let val3 = hashmap_get(hm, "key1");
    if val3 != 99 {
        return 3;
    }
    
    // Test missing key
    let val4 = hashmap_get(hm, "missing");
    if val4 != -1 {
        return 4;
    }
    
    hashmap_free(hm);
    return 0;
}
