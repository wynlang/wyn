// TDD Test: Regression test 496

fn test_regression_496() -> int {
    var x = 496
    var y = x * 2
    if y == 992 {
        print("PASS: regression 496\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 496 ===\n")
    var failures = test_regression_496()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
