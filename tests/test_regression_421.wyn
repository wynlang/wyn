// TDD Test: Regression test 421

fn test_regression_421() -> int {
    var x = 421
    var y = x * 2
    if y == 842 {
        print("PASS: regression 421\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 421 ===\n")
    var failures = test_regression_421()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
