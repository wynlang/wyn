// T3.1.1: Generic Functions Test - Simplified
// Tests basic generic programming support with type parameters

fn max[T](a, b) {
    if (a > b) {
        return a;
    }
    return b;
}

fn identity[T](value) {
    return value;
}

fn main() {
    print("=== T3.1.1 Generic Functions Test ===");
    
    // Test generic function with integers
    var int_max = max(5, 10);
    print("max(5, 10) = ", int_max);
    
    // Test identity function
    var id_result = identity(42);
    print("identity(42) = ", id_result);
    
    print("Generic functions test completed!");
    return 0;
}
