// TDD Test: Quality test 200

fn test_operation_200() -> int {
    var a = 200
    var b = 210
    var sum = a + b
    if sum == 410 {
        print("PASS: operation 200\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Quality Test 200 ===\n")
    var failures = test_operation_200()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
