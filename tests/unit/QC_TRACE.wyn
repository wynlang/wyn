fn main() -> int {
    let area = 10 * 20;
    let values = [100, 200, area];
    let selected = values[2];
    
    let result = match selected {
        200 => 1,
        _ => 2
    };
    
    let final_value = result;
    
    match final_value {
        2 => return 42,
        1 => return 99,
        _ => return 0
    }
}
