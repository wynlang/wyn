// TDD Test: Regression test 405

fn test_regression_405() -> int {
    var x = 405
    var y = x * 2
    if y == 810 {
        print("PASS: regression 405\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 405 ===\n")
    var failures = test_regression_405()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
