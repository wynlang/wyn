// TDD: JSON operations
// Tests creating JSON objects, setting values, and stringifying

fn main() -> int {
    // Test 1: Create JSON object and set string values
    var obj = json_new()
    json_set_string(obj, "name", "Alice")
    json_set_string(obj, "city", "Dubai")
    json_set_int(obj, "age", 30)

    // Test 2: Get values back
    println(json_get_string(obj, "name"))
    println(json_get_string(obj, "city"))
    println(json_get_int(obj, "age"))

    // Test 3: Stringify
    var s = json_stringify(obj)
    println(s)

    return 0
}
