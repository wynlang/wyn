// TDD Test: Regression test 480

fn test_regression_480() -> int {
    var x = 480
    var y = x * 2
    if y == 960 {
        print("PASS: regression 480\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 480 ===\n")
    var failures = test_regression_480()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
