// TDD Test: Regression test 489

fn test_regression_489() -> int {
    var x = 489
    var y = x * 2
    if y == 978 {
        print("PASS: regression 489\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 489 ===\n")
    var failures = test_regression_489()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
