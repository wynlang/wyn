// TDD Test: Regression test 407

fn test_regression_407() -> int {
    var x = 407
    var y = x * 2
    if y == 814 {
        print("PASS: regression 407\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 407 ===\n")
    var failures = test_regression_407()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
