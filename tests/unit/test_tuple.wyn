fn main() -> int {
    // Test basic tuple creation
    var pair = (10, 20);
    var x = pair.0;
    var y = pair.1;
    
    // Test tuple with 3 elements
    var triple = (1, 2, 3);
    var a = triple.0;
    var b = triple.1;
    var c = triple.2;
    
    // Test tuple with 4 elements
    var quad = (5, 6, 7, 8);
    var p = quad.0;
    var q = quad.1;
    var r = quad.2;
    var s = quad.3;
    
    // Return sum of all elements
    return x + y + a + b + c + p + q + r + s;
    // 10 + 20 + 1 + 2 + 3 + 5 + 6 + 7 + 8 = 62
}

