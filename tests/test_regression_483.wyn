// TDD Test: Regression test 483

fn test_regression_483() -> int {
    var x = 483
    var y = x * 2
    if y == 966 {
        print("PASS: regression 483\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 483 ===\n")
    var failures = test_regression_483()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
