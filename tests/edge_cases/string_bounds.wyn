fn main() -> int {
    print("=== String Bounds Edge Cases ===")
    
    var test_str = "Hello World"
    var len = wyn_string_len(test_str)
    print("Test string:")
    print(test_str)
    print("Length:")
    print(len)
    
    // Test substring with various bounds
    print("=== Substring Bounds Tests ===")
    
    // Normal case
    var sub1 = wyn_str_substring(test_str, 0, 5)
    print("substring(0, 5):")
    print(sub1)
    
    // Start beyond end
    var sub2 = wyn_str_substring(test_str, 20, 25)
    print("substring(20, 25) - start beyond end:")
    print(sub2)
    
    // End beyond length
    var sub3 = wyn_str_substring(test_str, 5, 50)
    print("substring(5, 50) - end beyond length:")
    print(sub3)
    
    // Negative start
    var sub4 = wyn_str_substring(test_str, -5, 5)
    print("substring(-5, 5) - negative start:")
    print(sub4)
    
    // Start > end
    var sub5 = wyn_str_substring(test_str, 8, 3)
    print("substring(8, 3) - start > end:")
    print(sub5)
    
    // Both negative
    var sub6 = wyn_str_substring(test_str, -10, -5)
    print("substring(-10, -5) - both negative:")
    print(sub6)
    
    // Start equals end
    var sub7 = wyn_str_substring(test_str, 5, 5)
    print("substring(5, 5) - start equals end:")
    print(sub7)
    
    // Start at exact length
    var sub8 = wyn_str_substring(test_str, len, len + 5)
    print("substring(len, len+5) - start at exact length:")
    print(sub8)
    
    print("=== Index Bounds Tests ===")
    
    // Test index_of with various cases
    var idx1 = wyn_string_index_of(test_str, "World")
    var idx2 = wyn_string_index_of(test_str, "NotFound")
    var idx3 = wyn_string_index_of(test_str, "")
    print("index_of('World'):")
    print(idx1)
    print("index_of('NotFound'):")
    print(idx2)
    print("index_of(''):")
    print(idx3)
    
    // Test last_index_of
    var repeated = "abcabcabc"
    var last_idx1 = wyn_string_last_index_of(repeated, "abc")
    var last_idx2 = wyn_string_last_index_of(repeated, "xyz")
    print("last_index_of('abc') in 'abcabcabc':")
    print(last_idx1)
    print("last_index_of('xyz') in 'abcabcabc':")
    print(last_idx2)
    
    print("=== Padding Bounds Tests ===")
    
    // Test padding with various widths
    var pad1 = wyn_string_pad_left(test_str, 5, "*")  // width < length
    var pad2 = wyn_string_pad_left(test_str, len, "*")  // width = length
    var pad3 = wyn_string_pad_left(test_str, 0, "*")   // width = 0
    var pad4 = wyn_string_pad_left(test_str, -5, "*")  // negative width
    
    print("pad_left width=5 (< length):")
    print(pad1)
    print("pad_left width=length:")
    print(pad2)
    print("pad_left width=0:")
    print(pad3)
    print("pad_left width=-5:")
    print(pad4)
    
    print("=== Repeat Bounds Tests ===")
    
    // Test repeat with various counts
    var rep1 = wyn_string_repeat("abc", 0)
    var rep2 = wyn_string_repeat("abc", -1)
    var rep3 = wyn_string_repeat("abc", 1)
    var rep4 = wyn_string_repeat("", 5)
    
    print("repeat('abc', 0):")
    print(rep1)
    print("repeat('abc', -1):")
    print(rep2)
    print("repeat('abc', 1):")
    print(rep3)
    print("repeat('', 5):")
    print(rep4)
    
    print("=== Split Edge Cases ===")
    
    // Test split with edge cases
    var count1 = split_count("", ",")
    var count2 = split_count("no-delimiter", ",")
    var count3 = split_count(",,,", ",")
    
    print("split('', ',') count:")
    print(count1)
    print("split('no-delimiter', ',') count:")
    print(count2)
    print("split(',,,', ',') count:")
    print(count3)
    
    print("=== String Bounds Tests Complete ===")
    return 0
}