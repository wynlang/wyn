// TDD Test: Regression test 425

fn test_regression_425() -> int {
    var x = 425
    var y = x * 2
    if y == 850 {
        print("PASS: regression 425\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Regression Test 425 ===\n")
    var failures = test_regression_425()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
