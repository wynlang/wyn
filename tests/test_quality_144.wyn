// TDD Test: Quality test 144

fn test_operation_144() -> int {
    var a = 144
    var b = 154
    var sum = a + b
    if sum == 298 {
        print("PASS: operation 144\n")
        return 0
    }
    return 1
}

fn main() -> int {
    print("=== Quality Test 144 ===\n")
    var failures = test_operation_144()
    if failures == 0 {
        print("\nAll tests PASSED!\n")
    }
    return failures
}
